plt:GenericPropertyOntology rdf:type voaf:Vocabulary , owl:Ontology ;
  dcterms:title "Platoon Generic Property Ontology"@en ;
  dcterms:description "The platoon generic property vocabulary for the  Platoon  project."@en ;
  dcterms:issued "2020-11-17"^^xsd:date ;
  dcterms:modified "2022-06-14"^^xsd:date ;
  dcterms:creator "Sarra BEN ABBES and Lynda TEMAL and Oumy SEYE" ; 
  dcterms:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "plt" ;
  vann:preferredNamespaceUri <https://w3id.org/platoon/> ;  
  owl:versionIRI <https://w3id.org/platoon/GenericPropertyOntology-1.7> ;
  owl:imports <https://w3id.org/saref> ,
              plt:ForecastingOntology-1.1,              
              seas:EvaluationOntology-1.0;           

  owl:versionInfo "v1.7" .

#######################################
# Comfort                             #
#######################################

plt:ComfortLevelProperty a owl:Class ;
  rdfs:label "Comfort Level Property"@en ;
  rdfs:comment """The class of properties that are quantifiable and have a quantity kind comfort level."""@en ;
  rdfs:subClassOf seas:Property;  
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:ComfortLevelEvaluation a owl:Class ;
  rdfs:label "Comfort Level Evaluation"@en ;
  rdfs:comment "The class of evaluation for Comfort Level properties"@en ;
  rdfs:subClassOf seas:Evaluation;  
  rdfs:subClassOf [ a owl:Restriction ; a owl:Restriction ;
            owl:onProperty seas:evaluationOf ; 
            owl:someValuesFrom plt:ComfortLevelProperty ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:ThresholdComfortLevelEvaluation a owl:Class ;
  rdfs:label "Threshold Comfort Level Evaluation"@en ;
  rdfs:comment "The class of evaluation for Threshold Comfort Level"@en ;
  rdfs:subClassOf plt:ComfortLevelEvaluation;  
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:MinimumThresholdComfortLevelEvaluation a owl:Class ;
  rdfs:label "Minimum Threshold Comfort Level Evaluation"@en ;
  rdfs:comment "The class of evaluation for Minimum Threshold Comfort Level"@en ;
  rdfs:subClassOf plt:ThresholdComfortLevelEvaluation;  
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:BasicComfortLevelEvaluation a owl:Class ;
  rdfs:label "Basic Comfort Level Evaluation"@en ;
  rdfs:comment "The class of evaluation for Basic Comfort Level"@en ;
  rdfs:subClassOf plt:ComfortLevelEvaluation;  
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:BestComfortLevelEvaluation a owl:Class ;
  rdfs:label "Best Comfort Level Evaluation"@en ;
  rdfs:comment "The class of evaluation for Best Comfort Level"@en ;
  rdfs:subClassOf plt:ComfortLevelEvaluation;  
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:hasComfortLevel a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has comfort Level"@en ;
  rdfs:comment """Links the feature of interest to its comfort Level ."""@en ;
  rdfs:subPropertyOf seas:hasProperty ;
  rdfs:domain seas:FeatureOfInterest;
  rdfs:range plt:ComfortLevelProperty ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.

plt:dependsOn a owl:ObjectProperty ;
  rdfs:label "depends on "@en ;
  rdfs:comment """Links a phenomen to its depending phenomen """@en ;
  rdfs:domain seas:FeatureOfInterest ;
  rdfs:range seas:FeatureOfInterest ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.
#######################################
# Occupancy                           #
#######################################
plt:OccupancyStatusEvaluation a owl:Class ;
  rdfs:label "Occupancy Status Evaluation"@en ;
  rdfs:comment "A class of evaluation for Occupancy "@en ;
  rdfs:subClassOf seas:Evaluation; 
  rdfs:subClassOf [ a owl:Restriction ; owl:onProperty seas:evaluationOf ; owl:someValuesFrom saref:Occupancy ] ;
  rdfs:subClassOf [ a owl:Restriction ; 
                    owl:onProperty seas:evaluatedSimpleValue; 
                    owl:allValuesFrom [ a rdfs:Datatype ; 
                                         owl:oneOf [ 
                                               a  rdf:List ;
                                               rdf:first  "&" ;
                                               rdf:rest   [ a          rdf:List ;
                                                    rdf:first  "Unoccupied" ;
                                                    rdf:rest   ()
                                              ] ] ] ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:OccupiedNumberEvaluation a owl:Class ;
  rdfs:label "Occupied Number Evaluation"@en ;
  rdfs:comment "A class of evaluation for Occupancy "@en ;
  rdfs:subClassOf seas:Evaluation; 
  rdfs:seeAlso  th:OccupiedStateValue;
  rdfs:subClassOf [ a owl:Restriction ; owl:onProperty seas:evaluationOf ; owl:someValuesFrom saref:Occupancy ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology .

plt:hasOccupancy a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has occupancy"@en ;
  rdfs:comment """Links the feature of interest to its occupancy."""@en ;
  rdfs:subPropertyOf seas:hasProperty ;
  rdfs:domain seas:FeatureOfInterest ;
  rdfs:range saref:Occupancy ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.

plt:hasEmployeesOccupancy a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has employees occupancy"@en ;
  rdfs:comment """Links the feature of interest to its employees occupancy."""@en ;
  rdfs:subPropertyOf plt:hasOccupancy ;
  rdfs:domain seas:FeatureOfInterest ;
  rdfs:range saref:Occupancy ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.

plt:hasClientsOccupancy a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has clients occupancy"@en ;
  rdfs:comment """Links the feature of interest to its clients occupancy."""@en ;
  rdfs:subPropertyOf plt:hasOccupancy ;
  rdfs:domain seas:FeatureOfInterest ;
  rdfs:range saref:Occupancy ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.

plt:ForecasterOfOccupancy a owl:Class ;
  rdfs:label "Forecast Of Occupancy"@en ;
  rdfs:comment "The class for forecast of occupancy"@en ;
  rdfs:subClassOf seas:Forecaster; 
  rdfs:subClassOf[ owl:onProperty seas:forecastsProperty ; owl:someValuesFrom saref:Occupancy ] ;
   rdfs:subClassOf[ owl:onProperty pep:made ; owl:someValuesFrom plt:ForecastOfOccupancy] ;
 vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.

plt:ForecastOfOccupancy a owl:Class ;
  rdfs:label "Forecast Of Occupancy"@en ;
  rdfs:comment "The class for forecast of occupancy"@en ;
  rdfs:subClassOf seas:Forecast; 
  rdfs:subClassOf[ owl:onProperty seas:forecastsProperty ; owl:someValuesFrom saref:Occupancy ] ;
   rdfs:subClassOf[ owl:onProperty pep:madeBY ; owl:someValuesFrom plt:ForecasterOfOccupancy] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.

plt:hasForecastOfOccupancy a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has Forecast Of Occupancy"@en ;
  rdfs:comment """Links the feature of interest to its occupancy forecast."""@en ;
  rdfs:subPropertyOf plt:hasForecastOfProperty;
  rdfs:domain seas:FeatureOfInterest;
  rdfs:range plt:ForecastOfOccupancy ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:GenericPropertyOntology.
