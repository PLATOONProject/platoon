plt:WeatherOntology rdf:type voaf:Vocabulary , owl:Ontology ;
  dcterms:title "Platoon Weather Ontology"@en ;
  dcterms:description "The Platoon Weather vocabulary for the  Platoon  project."@en ;
  dcterms:issued "2020-10-20"^^xsd:date ;
  dcterms:modified "2022-06-07"^^xsd:date ;
  dcterms:creator "Sarra BEN ABBES and Lynda TEMAL and Oumy SEYE " ; 
  dcterms:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "plt" ;
  vann:preferredNamespaceUri <https://w3id.org/platoon/> ;  
  owl:versionIRI <https://w3id.org/platoon/WeatherOntology-1.6> ;
  owl:imports <https://w3id.org/saref> ,
              <https://w3id.org/bot> ,
              <https://w3id.org/seas/seas-1.1>;     
  owl:versionInfo "v1.6" .
#######################################
# Weather Station                     #
#######################################

plt:WeatherStation a owl:Class ;
  rdfs:label "Weather Station"@en ;
  rdfs:comment """A weather station is  instruments and equipment for measuring atmospheric conditions to provide information for weather forecasts and to study the weather and climate.(source : wikipedia) """@en ;
  rdfs:subClassOf seas:System ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:WeatherOntology .

#######################################
# Air Temperature                     #
#######################################

plt:WeatherProperty a owl:Class ;
  rdfs:label "Weather  Property"@en ;
  rdfs:comment "The class of properties that are quantifiable and have a quantity kind of Weather measure."@en ;
   rdfs:subClassOf   seas:Property;  
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:WeatherOntology .

plt:AirTemperatureProperty a owl:Class ;
  rdfs:label "Air Temperature Property"@en ;
  rdfs:comment "Quantity of air temperature expressed in Kelvin, DegreeCelcius of Farenheit of some zone"@en ;
  rdfs:subClassOf seas:TemperatureProperty;
  rdfs:subClassOf [ a owl:Restriction ; owl:onProperty saref:isPropertyOf ; owl:someValuesFrom bot:Zone ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:WeatherOntology .

plt:AirTemperatureEvaluation a owl:Class ;
  rdfs:label "Air Temperature Evaluation"@en ;
  rdfs:comment """The class of evaluation for air temperature properties 
                    example : <zone/1/airTemperatureEvaluation> a plt:AirTemperatureEvaluation ;
                                     seas:evaluatedSimpleValue 17.00 ;
                                     qudt:unit unit:DEG_C ]"""@en ;
  rdfs:subClassOf seas:TemperatureEvaluation;  
  rdfs:subClassOf [ a owl:Restriction ; owl:onProperty seas:evaluationOf ; owl:someValuesFrom plt:AirTemperatureProperty ] ;
  owl:equivalentClass [ owl:onProperty seas:evaluationOf ; owl:someValuesFrom plt:AirTemperatureProperty ] ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:WeatherOntology .

plt:hasAirTemperature a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has  air temperature"@en ;
  rdfs:comment """Links the feature of interest to its  air  temperature."""@en ;
  rdfs:subPropertyOf seas:temperature ;
  rdfs:domain seas:FeatureOfInterest ;
  rdfs:range plt:AirTemperatureProperty ;
  vs:term_status "testing" ;
  rdfs:isDefinedBy plt:WeatherOntology  .
  
plt:hasIndoorAirTemperature a owl:ObjectProperty , owl:FunctionalProperty ;
  rdfs:label "has indoor air temperature"@en ;
